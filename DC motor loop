// this is just a loop that turns a DC motor on and off. Pretty simple. It also has an LED that turns on and 
// off. It is optional 
// created by Brody Pennington on 7/18/2020 

int DC1 = 12;
int LED1 = 13;


void setup() {
  pinMode(DC1, OUTPUT);
}

void loop() {
  digitalWrite(DC1, HIGH);
  digitalWrite(LED1, HIGH);
  delay(5000);
  digitalWrite(DC1, LOW);
  digitalWrite(LED1, LOW);
  delay(2000);

}
