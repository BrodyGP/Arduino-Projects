int LEDB1 = 13;
int LEDB2 = 12;
int LEDB3 = 11;
int LEDW1 = 10;
int LEDW2 = 9;
int LEDW3 = 8;
int LEDB4 = 7;
int LEDB5 = 6;
int LEDB6 = 5;
int LEDW4 = 4;
int LEDW5 = 3;
int LEDW6 = 2;

void setup() {
  pinMode(LEDB1, OUTPUT);
  pinMode(LEDB2, OUTPUT);
  pinMode(LEDB3, OUTPUT);
  pinMode(LEDW1, OUTPUT);
  pinMode(LEDW2, OUTPUT);
  pinMode(LEDW3, OUTPUT);
  pinMode(LEDB4, OUTPUT);
  pinMode(LEDB5, OUTPUT);
  pinMode(LEDB6, OUTPUT);
  pinMode(LEDW4, OUTPUT);
  pinMode(LEDW5, OUTPUT);
  pinMode(LEDW6, OUTPUT);
}

void loop() {
  digitalWrite(LEDB1, HIGH);
  digitalWrite(LEDB2, HIGH);
  digitalWrite(LEDB3, HIGH);
  digitalWrite(LEDB4, HIGH);
  digitalWrite(LEDB5, HIGH);
  digitalWrite(LEDB6, HIGH);
  delay(100);
  digitalWrite(LEDB1, LOW);
  digitalWrite(LEDB2, LOW);
  digitalWrite(LEDB3, LOW);
  digitalWrite(LEDB4, LOW);
  digitalWrite(LEDB5, LOW);
  digitalWrite(LEDB6, LOW);
  delay(100);
  digitalWrite(LEDW1, HIGH);
  digitalWrite(LEDW2, HIGH);
  digitalWrite(LEDW3, HIGH);
  digitalWrite(LEDW4, HIGH);
  digitalWrite(LEDW5, HIGH);
  digitalWrite(LEDW6, HIGH);
  delay(100);
  digitalWrite(LEDW1, LOW);
  digitalWrite(LEDW2, LOW);
  digitalWrite(LEDW3, LOW);
  digitalWrite(LEDW4, LOW);
  digitalWrite(LEDW5, LOW);
  digitalWrite(LEDW6, LOW);
  delay(100);
  digitalWrite(LEDB1, HIGH);
  digitalWrite(LEDB2, HIGH);
  digitalWrite(LEDB3, HIGH);
  digitalWrite(LEDB4, HIGH);
  digitalWrite(LEDB5, HIGH);
  digitalWrite(LEDB6, HIGH);
  delay(100);
  digitalWrite(LEDB1, LOW);
  digitalWrite(LEDB2, LOW);
  digitalWrite(LEDB3, LOW);
  digitalWrite(LEDB4, LOW);
  digitalWrite(LEDB5, LOW);
  digitalWrite(LEDB6, LOW);
  delay(100);
  digitalWrite(LEDW1, HIGH);
  digitalWrite(LEDW2, HIGH);
  digitalWrite(LEDW3, HIGH);
  digitalWrite(LEDW4, HIGH);
  digitalWrite(LEDW5, HIGH);
  digitalWrite(LEDW6, HIGH);
  delay(100);
  digitalWrite(LEDW1, LOW);
  digitalWrite(LEDW2, LOW);
  digitalWrite(LEDW3, LOW);
  digitalWrite(LEDW4, LOW);
  digitalWrite(LEDW5, LOW);
  digitalWrite(LEDW6, LOW);
  delay(100);
  digitalWrite(LEDB1, HIGH);
  digitalWrite(LEDB2, HIGH);
  digitalWrite(LEDB3, HIGH);
  digitalWrite(LEDB4, HIGH);
  digitalWrite(LEDB5, HIGH);
  digitalWrite(LEDB6, HIGH);
  delay(100);
  digitalWrite(LEDB1, LOW);
  digitalWrite(LEDB2, LOW);
  digitalWrite(LEDB3, LOW);
  digitalWrite(LEDB4, LOW);
  digitalWrite(LEDB5, LOW);
  digitalWrite(LEDB6, LOW);
  delay(100);
  digitalWrite(LEDW1, HIGH);
  digitalWrite(LEDW2, HIGH);
  digitalWrite(LEDW3, HIGH);
  digitalWrite(LEDW4, HIGH);
  digitalWrite(LEDW5, HIGH);
  digitalWrite(LEDW6, HIGH);
  delay(100);
  digitalWrite(LEDW1, LOW);
  digitalWrite(LEDW2, LOW);
  digitalWrite(LEDW3, LOW);
  digitalWrite(LEDW4, LOW);
  digitalWrite(LEDW5, LOW);
  digitalWrite(LEDW6, LOW);
  delay(200);
  digitalWrite(LEDB1, HIGH);
  digitalWrite(LEDB2, HIGH);
  digitalWrite(LEDB3, HIGH);
  digitalWrite(LEDB4, HIGH);
  digitalWrite(LEDB5, HIGH);
  digitalWrite(LEDB6, HIGH);
  digitalWrite(LEDW1, HIGH);
  digitalWrite(LEDW2, HIGH);
  digitalWrite(LEDW3, HIGH);
  digitalWrite(LEDW4, HIGH);
  digitalWrite(LEDW5, HIGH);
  digitalWrite(LEDW6, HIGH);
  delay(200);
  digitalWrite(LEDB1, LOW);
  digitalWrite(LEDB2, LOW);
  digitalWrite(LEDB3, LOW);
  digitalWrite(LEDB4, LOW);
  digitalWrite(LEDB5, LOW);
  digitalWrite(LEDB6, LOW);
  digitalWrite(LEDW1, LOW);
  digitalWrite(LEDW2, LOW);
  digitalWrite(LEDW3, LOW);
  digitalWrite(LEDW4, LOW);
  digitalWrite(LEDW5, LOW);
  digitalWrite(LEDW6, LOW);
  delay(200);
  digitalWrite(LEDB1, HIGH);
  digitalWrite(LEDB2, HIGH);
  digitalWrite(LEDB3, HIGH);
  digitalWrite(LEDB4, HIGH);
  digitalWrite(LEDB5, HIGH);
  digitalWrite(LEDB6, HIGH);
  digitalWrite(LEDW1, HIGH);
  digitalWrite(LEDW2, HIGH);
  digitalWrite(LEDW3, HIGH);
  digitalWrite(LEDW4, HIGH);
  digitalWrite(LEDW5, HIGH);
  digitalWrite(LEDW6, HIGH);
  delay(200);
  digitalWrite(LEDB1, LOW);
  digitalWrite(LEDB2, LOW);
  digitalWrite(LEDB3, LOW);
  digitalWrite(LEDB4, LOW);
  digitalWrite(LEDB5, LOW);
  digitalWrite(LEDB6, LOW);
  digitalWrite(LEDW1, LOW);
  digitalWrite(LEDW2, LOW);
  digitalWrite(LEDW3, LOW);
  digitalWrite(LEDW4, LOW);
  digitalWrite(LEDW5, LOW);
  digitalWrite(LEDW6, LOW);
  delay(200);
}
